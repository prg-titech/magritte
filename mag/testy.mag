load mag/prelude.mag

(
  put [a 1]
  put [b 2]
  put [a 3]
  put [b 4]
) | each (
  [a ?x] => put [a (mul 10 $x)]
  [b ?y] => put [b (mul 100 $y)]
)

iter ([a ?x] => put [b (add 2 $x)]; [b ?y] => put [a (mul 3 $y)]) [a 1]

